# Configurações da API Autentique
VITE_AUTENTIQUE_API_URL=https://api.autentique.com.br/v2/graphql
VITE_AUTENTIQUE_API_KEY=your_api_key_here

# Modo de desenvolvimento - forçar uso de dados mockados
# Defina como 'true' para usar sempre dados simulados em desenvolvimento
# Útil quando a API Autentique não está acessível ou para testes
VITE_USE_MOCK_AUTENTIQUE=false

# Configurações do Supabase
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Outras configurações
VITE_APP_NAME=SCI-Core
VITE_APP_VERSION=1.0.0